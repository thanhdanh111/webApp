{
  "name": "stn-app",
  "version": "0.1.0",
  "private": true,
  "sasslintConfig": ".sasslintrc.json",
  "scripts": {
    "local": "nodemon -e ts,tsx,sass,js",
    "start:prod": "export NODE_ENV=production && next build && next export && serve out",
    "start:staging": "export NODE_ENV=staging && next build && next export && serve out",
    "start": "export NODE_ENV=local && next build && next export && serve out",
    "static": "serve out",
    "debug": "export NODE_ENV=local && next dev -p 5000",
    "build": "NODE_ENV=production next build && next export",
    "build:staging": "NODE_ENV=staging next build && next export",
    "test": "NODE_ENV=test jest",
    "tslint": "tslint -p . -c tslint.json",
    "sasslint": "sass-lint -v -q",
    "linter-folder": "ts-node --project tsconfig.server.json linter/linter_folder.ts",
    "storybook": "start-storybook -p 6006 -c .storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^4.0.0-alpha.12",
    "@sentry/browser": "^5.15.5",
    "@sentry/react": "^6.0.4",
    "@storybook/react": "^5.3.18",
    "@types/react-redux": "7.1.16",
    "@zeit/next-sass": "^1.0.1",
    "apexcharts": "^3.26.0",
    "axios": "^0.19.2",
    "date-fns": "^2.22.1",
    "directory-tree": "^2.2.4",
    "dotenv": "^8.2.0",
    "firebase": "^8.3.3",
    "jest": "^24.9.0",
    "material-ui-popup-state": "^1.8.0",
    "moment": "^2.29.1",
    "next": "10.0.5",
    "next-redux-wrapper": "^5.0.0",
    "nextjs-fonts": "^0.22.0",
    "node-sass": "^5.0.0",
    "notistack": "^1.0.5",
    "query-string": "^4.3.4",
    "react": "17.0.1",
    "react-apexcharts": "1.3.7",
    "react-dom": "17.0.1",
    "react-infinite-scroll-component": "^6.0.0",
    "react-loadingg": "^1.7.2",
    "react-redux": "7.2.3",
    "react-router-dom": "^5.2.0",
    "react-waypoint": "^9.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@percy/puppeteer": "^1.1.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^17.0.0",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "jest-image-snapshot": "^4.0.0",
    "nodemon": "^2.0.7",
    "puppeteer": "^3.0.3",
    "redux-devtools-extension": "^2.13.8",
    "sass-lint": "^1.13.1",
    "serve": "^11.3.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run linter-folder && npm run tslint && npm run sasslint"
    }
  }
}
